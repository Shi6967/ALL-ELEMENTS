local function getStrengthRequiredForRebirth(c)
Â  Â  local rebirths = c.leaderstats.Rebirths.Value
Â  Â  local baseStrength = 10000 + (5000 * rebirths)
Â  Â Â 
Â  Â  local g = c:FindFirstChild("ultimatesFolder")
Â  Â  local golden = 0
Â  Â  if g and g:FindFirstChild("Golden Rebirth") then
Â  Â  Â  Â  golden = g["Golden Rebirth"].Value
Â  Â  end
Â  Â Â 
Â  Â  if golden >= 1 and golden <= 5 then
Â  Â  Â  Â  baseStrength = baseStrength * (1 - golden * 0.1)
Â  Â  end
Â  Â  return math.floor(baseStrength)
end

local function hitRock()
